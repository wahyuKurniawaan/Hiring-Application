<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arkademy.wahyu.hiringapplication">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_businessman"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_businessman_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.OnboardActivity" />
        <activity android:name=".activity.ProfileActivity" />
        <activity android:name=".activity.FormInputActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>